{
  "name": "eslint-config-artdeco",
  "version": "1.0.0",
  "description": "An eslint shareable config for Art Deco Code.",
  "main": "build",
  "scripts": {
    "test": "zoroaster test/spec --babel",
    "test-build": "BABEL_ENV=test-build zoroaster test/spec --babel",
