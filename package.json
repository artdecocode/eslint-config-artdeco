  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artdecocode/eslint-config-artdeco/issues"
  },
  "homepage": "https://github.com/artdecocode/eslint-config-artdeco#readme",
  "devDependencies": {
    "zoroaster": "2.1.0"
  }
}
